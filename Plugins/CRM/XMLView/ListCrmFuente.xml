<view>
    <columns>
        <column name="code" display="none" order="100">
            <widget type="text" fieldname="id" />
        </column>
        <column name="name" order="110">
            <widget type="text" fieldname="nombre" />
        </column>
        <column name="description" order="120">
            <widget type="textarea" fieldname="descripcion" />
        </column>
        <column name="contacts" display="right" order="130">
            <widget type="number" decimal="0" fieldname="numcontactos" />
        </column>
        <column name="date" display="right" order="140">
            <widget type="date" fieldname="fecha" />
        </column>
    </columns>
</view>