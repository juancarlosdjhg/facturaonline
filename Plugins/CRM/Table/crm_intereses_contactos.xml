<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : crm_intereses_contactos.xml
    Description:
        Estructura de la tabla crm_intereses_contactos.
-->
<table>
    <column>
        <name>fecha</name>
        <type>date</type>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>idcontacto</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>idinteres</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>crm_intereses_contactos_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>ca_crm_intereses_contacto</name>
        <type>FOREIGN KEY (idcontacto) REFERENCES contactos (idcontacto) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_crm_intereses_interes</name>
        <type>FOREIGN KEY (idinteres) REFERENCES crm_intereses (id) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>
